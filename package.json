{
  "name": "gh-pages-deploy-sample",
  "version": "1.0.0",
  "description": "Basic project to practice gh-pages deploy automation",
  "main": "index.js",
  "config": {
    "deployFolder": "dist",
    "deployRemote": "GH-FJCalzado"
  },
  "scripts": {
    "start": "webpack-dev-server --inline",
    "clean": "rimraf \"$npm_package_config_deployFolder/!(.git)*\"",
    "build": "npm run clean && webpack",
    "ghDeploy": "npm run build && cd $npm_package_config_deployFolder && git add --all && git commit -m \"GH Deploy $(date \"+%s\")\" && git push && cd.."
  },
  "author": "Javier Calzado",
  "license": "MIT",
  "dependencies": {
    "bootstrap": "~3.3.7",
    "react": "~16.1.1",
    "react-dom": "~16.1.1"
  },
  "devDependencies": {
    "@types/react": "^16.0.5",
    "@types/react-dom": "^16.0.3",
    "awesome-typescript-loader": "^3.2.3",
    "babel-core": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "css-loader": "^0.28.7",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^1.1.5",
    "html-webpack-plugin": "^2.30.1",
    "rimraf": "^2.6.2",
    "style-loader": "^0.19.0",
    "typescript": "^2.5.2",
    "url-loader": "^0.6.2",
    "webpack": "^3.6.0",
    "webpack-dev-server": "^2.8.2"
  }
}
